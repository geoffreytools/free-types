{
    "name": "free-types",
    "version": "0.1.1",
    "description": "A type-level library enabling the creation and the manipulation of type constructors which are detached from their type parameters.",
    "keywords:": [
        "higher kinded type",
        "type constructor",
        "typescript"
    ],
    "exports": {
        ".": {
            "import": "./index.ts"
        },
        "./core": {
            "import": "./core/index.ts"
        },
        "./essential": {
            "import": "./essential/index.ts"
        },
        "./utility-types": {
            "import": "./utility-types/index.ts"
        },
        "./experimental": {
            "import": "./experimental/index.ts"
        }
    },
    "types": "./index.ts",
    "author": "Geoffrey Gourdet",
    "repository": {
        "type": "git",
        "url": "https://github.com/geoffreyTools/free-types"
    },
    "license": "ICS",
    "devDependencies": {
        "foo": "github:geoffreytools/foo",
        "ts-spec": "^1.0.2",
        "ts-trace": "github:geoffreytools/ts-trace",
        "typescript": "^4.8.4"
    },
    "scripts": {
        "test": "tsc",
        "test: watch": "tsc --watch",
        "trace": "bash ./node_modules/ts-trace/ts-trace.sh"
    },
    "dependencies": {
        "free-types-core": "^0.3.0"
    }
}
