{
    "name": "free-types",
    "version": "0.2.4",
    "description": "A type-level library enabling the creation and the manipulation of type constructors which are detached from their type parameters.",
    "keywords:": [
        "higher kinded type",
        "type constructor",
        "typescript"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./core": {
            "import": "./dist/core/index.js"
        },
        "./essential": {
            "import": "./dist/essential/index.js"
        },
        "./utility-types": {
            "import": "./dist/utility-types/index.js"
        },
        "./experimental": {
            "import": "./dist/experimental/index.js"
        }
    },
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "files": [
        "dist/*"
    ],
    "author": "Geoffrey Gourdet",
    "repository": {
        "type": "git",
        "url": "https://github.com/geoffreyTools/free-types"
    },
    "license": "ICS",
    "devDependencies": {
        "foo": "github:geoffreytools/foo",
        "ts-spec": "^1.0.2",
        "ts-trace": "github:geoffreytools/ts-trace",
        "typescript": "^4.8.4"
    },
    "scripts": {
        "test": "tsc",
        "test: watch": "tsc --watch",
        "build": "tsc -p ./tsconfig-build.json",
        "trace": "bash ./node_modules/ts-trace/ts-trace.sh"
    },
    "dependencies": {
        "free-types-core": "^0.4.2"
    }
}
